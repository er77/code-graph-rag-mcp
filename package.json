{
  "name": "@er77/code-graph-rag-mcp",
  "version": "2.7.18",
  "description": "Multi-agent LiteRAG MCP server for advanced code graph analysis",
  "license": "MIT",
  "author": "er77",
  "homepage": "https://github.com/er77/code-graph-rag-mcp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/er77/code-graph-rag-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/er77/code-graph-rag-mcp/issues"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "code-analysis",
    "graph",
    "rag",
    "llm",
    "agents",
    "literag",
    "typescript",
    "sqlite",
    "vector-search",
    "semantic-search",
    "claude",
    "tree-sitter"
  ],
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "code-graph-rag-mcp": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "PERFORMANCE_GUIDE.md",
    "SQLITE_VEC_INSTALLATION.md",
    "CLAUDE.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules PARSER_DISABLE_CACHE=1 node scripts/run-tests.js",
    "test:quiet": "cross-env NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test node scripts/run-tests.js --silent",
    "test:verbose": "cross-env NODE_OPTIONS=--experimental-vm-modules node scripts/run-tests.js --verbose",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules node scripts/run-tests.js --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules node scripts/run-tests.js --coverage",
    "smoke": "node scripts/smoke-graph-health.js /home/er77/_work_fodler/baserow-develop",
    "smoke:clean": "node scripts/smoke-graph-health.js /home/er77/_work_fodler/baserow-develop --clean",
    "smoke:semantic": "node scripts/smoke-semantic.js /home/er77/_work_fodler/baserow-develop",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged && npm run typecheck"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md,yml,yaml}": "biome check --write"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "latest",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "cross-env": "^10.1.0",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.4",
    "simple-git-hooks": "^2.13.1",
    "ts-jest": "^29.4.4",
    "tsup": "^8.3.6",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@er77/code-graph-rag-mcp": "^2.5.9",
    "@modelcontextprotocol/sdk": "^1.5.0",
    "better-sqlite3": "^11.10.0",
    "lru-cache": "^10.0.0",
    "nanoid": "^5.0.0",
    "sqlite-vec": "^0.1.6",
    "tree-sitter": "0.21.1",
    "tree-sitter-c": "0.23.2",
    "tree-sitter-c-sharp": "0.21.3",
    "tree-sitter-cpp": "0.23.4",
    "tree-sitter-go": "0.23.4",
    "tree-sitter-java": "0.23.5",
    "tree-sitter-javascript": "0.23.0",
    "tree-sitter-kotlin": "^0.3.8",
    "tree-sitter-python": "0.23.3",
    "tree-sitter-rust": "0.21.0",
    "tree-sitter-typescript": "0.23.2",
    "yaml": "^2.8.1",
    "zod": "^3.23.0",
    "zod-to-json-schema": "^3.23.0"
  },
  "optionalDependencies": {
    "@xenova/transformers": "^2.17.2"
  },
  "peerDependencies": {
    "onnxruntime-node": "^1.23.0"
  },
  "peerDependenciesMeta": {
    "onnxruntime-node": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "notes": {}
}
